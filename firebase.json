{
  "hosting": {
    "rewrites": [ 
      {
        "source": "/api/**",
        "run": {
          "serviceId": "fullstack-challenge",  
          "region": "us-central1"   
        }
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
    
  }
}
